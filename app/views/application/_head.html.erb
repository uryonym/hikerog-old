<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title><%= full_title(yield(:title)) %></title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag "https://cdnjs.cloudflare.com/ajax/libs/uikit/2.27.2/css/uikit.min.css" %>
  <%= stylesheet_link_tag "https://cdnjs.cloudflare.com/ajax/libs/uikit/2.27.2/css/components/form-password.min.css" %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag "https://cdnjs.cloudflare.com/ajax/libs/uikit/2.27.2/js/uikit.min.js" %>
  <%= javascript_include_tag "https://cdnjs.cloudflare.com/ajax/libs/uikit/2.27.2/js/components/form-password.min.js" %>
  <%= csrf_meta_tags %>
</head>